{
  "name": "client_web",
  "license": "GPL-3.0",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron.js",
  "scripts": {
    "start": "mkcert -install && mkcert localhost && vite",
    "dev": "vite --host",
    "production": "vite --mode production",
    "build": "tsc -b && vite build",
    "lint": "tsc --noEmit -p tsconfig.json",
    "preview": "vite preview",
    "electron": "electron .",
    "dist": "electron-builder",
    "dist:linux": "electron-builder --linux",
    "dist:win": "electron-builder --win",
    "test": "jest --silent",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "build": {
    "appId": "com.area.id",
    "productName": "AREA",
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Utility",
      "icon": "public/area.png"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/area.png"
    }
  },
  "description": "Connect different services in one place",
  "author": {
    "email": "maintainer@email.com",
    "name": "maintainer",
    "url": "https://github.com/ASM-Studios/AREA"
  },
  "homepage": "https://github.com/ASM-Studios/AREA",
  "icon": "public/area.png",
  "dependencies": {
    "@azure/msal-browser": "^3.27.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@react-oauth/google": "^0.12.1",
    "@tsparticles/engine": "^3.7.1",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.7.1",
    "@types/js-cookie": "^3.0.6",
    "antd": "^5.22.1",
    "axios": "^1.7.7",
    "cobe": "^0.6.3",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.11.17",
    "js-cookie": "^3.0.5",
    "react": "^18.3.1",
    "react-color": "^2.19.3",
    "react-dom": "^18.3.1",
    "react-responsive": "^10.0.0",
    "react-router-dom": "^6.28.0",
    "react-toastify": "^10.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-responsive": "^8.0.8",
    "@vitejs/plugin-react": "^4.3.3",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "public",
        "staticOutDir": "public"
      }
    ]
  }
}
