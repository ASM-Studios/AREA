{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@Config/*": ["src/Config/*"]
    },
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "target": "es2015",
    "noEmit": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
  },
  "include": ["src"]
}
